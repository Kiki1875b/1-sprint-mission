{
  "family": "discodeit-task",
  "taskRoleArn": "arn:aws:iam::061039791550:role/ecsTaskExecutionRole",
  "executionRoleArn": "arn:aws:iam::061039791550:role/ecsTaskExecutionRole",
  "networkMode": "bridge",
  "requiresCompatibilities": [
    "EC2"
  ],
  "cpu": "256",
  "memory": "512",
  "containerDefinitions": [
    {
      "name": "discodeit-app",
      "image": "replace-me",
      "cpu": 256,
      "memory": 512,
      "memoryReservation": 256,
      "essential": true,
      "portMappings": [
        {
          "name": "discodeit-app-80-tcp",
          "containerPort": 80,
          "hostPort": 80,
          "protocol": "tcp",
          "appProtocol": "http"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/discodeit-task",
          "awslogs-create-group": "true",
          "awslogs-region": "ap-northeast-2",
          "awslogs-stream-prefix": "ecs",
          "mode": "non-blocking",
          "max-buffer-size": "25m"
        }
      }
    }
  ]
}
